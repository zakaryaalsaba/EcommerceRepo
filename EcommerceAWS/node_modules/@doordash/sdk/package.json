{
  "name": "@doordash/sdk",
  "version": "0.6.12",
  "description": "The DoorDash Node.js SDK with the ability to request a delivery through the Drive API",
  "keywords": [
    "doordash",
    "drive",
    "api",
    "delivery",
    "logistics"
  ],
  "homepage": "https://developer.doordash.com/",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "DoorDash",
  "contributor": [
    "Yang Yang"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doordash"
  },
  "exports": {
    "require": "./lib/index.js",
    "import": "./lib/index.js"
  },
  "scripts": {
    "generate": "rm -rf src/generated-client && openapi-generator generate -i node_modules/@doordash/api-spec-drive/dist/drive-v2-openapi.yaml -o src/generated-client -g typescript-axios",
    "build": "rm -rf lib && tsc"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "@types/uuid": "^8.3.3",
    "axios": "^0.24.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.6",
    "@doordash/api-spec-drive": "1.0.21",
    "typescript": "^4.5.4"
  }
}
